{
  "compilerOptions": {
    "sourceMap": true,
    "noImplicitAny": false,
    "module": "ESNext",
    "moduleResolution": "Node",
    "target": "ES6",
    "removeComments": false,
    "allowSyntheticDefaultImports": true,
    "jsx": "react",
    "allowJs": true,
    "esModuleInterop": true,
    "baseUrl": "./",
    "paths": {
      "components/*": [
        "src/components/*"
      ],
    }
  }
}